{
  "name": "mobx-react-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for MobX + React project with Typescript, ES6 compilation and hot code reloading",
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false craco start",
    "build": "craco build",
    "test": "craco test",
    "lint:check": "eslint --ext \".ts,.tsx\" \"src/**/*.{ts,tsx,*.ts,*tsx}\"",
    "lint:format": "eslint --fix --ext \".ts,.tsx\" \"src/**/*.{ts,tsx,*.ts,*tsx}\"",
    "ts:check": "tsc --project ./tsconfig.json --skipLibCheck --noemit",
    "style:check": "stylelint \"src/**/*.{css,scss}\"",
    "style:format": "stylelint --fix \"src/**/*.{css,scss}\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spanderok/todo-local"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "mobx",
    "starter-kit"
  ],
  "overrides": {
    "glob-parent": "5.1.2",
    "nth-check": "2.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@lomray/eslint-config-react": "^2.0.0",
    "@lomray/prettier-config": "^1.2.0",
    "@lomray/stylelint-config": "^1.3.0",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jwt-encode": "^1.0.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.isnil": "^4.0.7",
    "@types/node": "^18.0.3",
    "@types/node-sass": "^4.11.3",
    "@types/react": "^18.0.15",
    "@types/react-collapse": "^5.0.1",
    "@types/react-dom": "^18.0.6",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-select-country-list": "^2.2.0",
    "@types/react-star-ratings": "^2.3.0",
    "@types/uuid": "^8.3.4",
    "copy-webpack-plugin": "^11.0.0",
    "craco": "^0.0.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "node-sass": "^7.0.1",
    "prettier": "^2.7.1",
    "react-app-alias": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "semantic-release": "^19.0.5",
    "stylelint": "^14.13.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@npmcli/fs": "^3.1.0",
    "antd": "^4.16.13",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.2",
    "firebase": "^9.9.4",
    "formik": "^2.2.9",
    "hoist-non-react-statics": "^3.3.2",
    "inversify": "^5.1.1",
    "jwt-encode": "^1.0.1",
    "lodash.debounce": "^4.0.8",
    "lodash.isnil": "^4.0.0",
    "mobx": "^6.6.1",
    "mobx-react-lite": "^3.4.0",
    "react": "^18.2.0",
    "react-collapse": "^5.1.1",
    "react-cookie-consent": "^8.0.1",
    "react-date-picker": "^9.0.0",
    "react-dom": "^18.2.0",
    "react-easy-crop": "^4.5.0",
    "react-ga": "^3.3.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-notifications-component": "^4.0.1",
    "react-phone-number-input": "^3.2.11",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.4.0",
    "react-simple-star-rating": "^5.1.2",
    "react-transition-group": "^4.4.5",
    "rxjs": "^7.8.0",
    "sass": "^1.57.1",
    "swiper": "^8.3.2",
    "tippy.js": "^6.2.5",
    "universal-cookie": "^4.0.4",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
