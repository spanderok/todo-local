    "css-loader": "^5.2.4",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.1",
    "prettier": "^2.3.2",
    "style-loader": "^2.0.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-dev-server": "^3.8.2"
  },
  "dependencies": {
    "antd": "^4.16.13",
    "inversify": "^5.1.1",
    "mobx": "6.3.0",
